{
  "name": "zachurich.com",
  "version": "1.0.0",
  "description": "zach urich's website",
  "main": "server.js",
  "scripts": {
    "dev": "next dev",
    "dev:server": "node server.js",
    "build": "NODE_ENV=production next build",
    "prod:server": "NODE_ENV=production node ./server.js",
    "prod": "npm run build && npm run prod:server",
    "deploy": "now rm zachurich.com -y && now",
    "alias": "now alias",
    "export:dev": "npm run build && next export && cd out && serve -p 8080",
    "export:prod": "npm run build && next export && cd out"
  },
  "dependencies": {
    "autoprefixer": "^6.4.1",
    "axios": "^0.16.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^6.3.2",
    "babel-plugin-inline-react-svg": "^0.5.3",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-wrap-in-js": "^1.1.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.26.2",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.9.0",
    "glob": "^7.1.2",
    "next": "^6.1.1",
    "node-sass": "^4.9.0",
    "nodemailer": "^4.6.7",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "postcss-easy-import": "^3.0.0",
    "postcss-loader": "^2.1.5",
    "prismic-javascript": "^1.5.0",
    "prismic-reactjs": "^0.3.2",
    "raw-loader": "^0.5.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-ga": "^2.5.3",
    "react-share": "^1.19.1",
    "react-svg": "^2.2.18",
    "react-transition-group": "^1.2.1",
    "sass-loader": "^4.0.2",
    "webpack": "^2.7.0",
    "whatwg-fetch": "^2.0.4",
    "xss-filters": "^1.2.7"
  },
  "devDependencies": {
    "lodash": "^4.17.11"
  },
  "author": "Zach Urich"
}
